public class Main{
    public static void main(String args[]){
        A a= new A();
        B b= new B();
        System.out.println(a.aName);
        a.aDisplay();
         System.out.println(b.bName);
        b.bDisplay();
        System.out.println(b.aName);
        b.aDisplay();
    }
}
class A {
    String aName="A";
    void aDisplay(){
       System.out.println("in A");
    }
}
class B extends A {
    String bName="B";
    void bDisplay(){
        System.out.println("in B");
    }
}                                                                                                                                      